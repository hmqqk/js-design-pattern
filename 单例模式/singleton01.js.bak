/**
 * 单例模式在JS中使用非常的频繁
 * 通过确保单例对象只存在一个实例,
 * 你就可以确信自己在所有的代码中使用的是全局资源
 */
(function(){
	//先看来一个最简单的单体
	//例如用户登录后的信息可以用一个单体存储
	var UserInfo = {
		name:"liuhong",
		code:"666666",
		deptName:'cs',
		deptCode:'cs666',
		getName : function(){
			return "liuhong";
		}
	}
	alert(UserInfo.getName());
	
	var comm = {};
	comm.UserInfo = {
		name:"USPCAT.COM",
		code:"00101"		
	}
	comm.funcInfo ={
		funcName:'',
		funcCode:""
	}

})()



